{
  "name": "graphql-server-servie",
  "version": "0.3.0",
  "description": "Production-ready Node.js GraphQL server for Servie",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\" --project tsconfig.json",
    "build": "rm -rf dist/ && tsc",
    "specs": "jest --coverage",
    "test": "npm run -s lint && npm run -s build && npm run -s specs",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/serviejs/graphql-server-servie.git"
  },
  "keywords": [
    "servie",
    "graphql",
    "apollo",
    "server"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/serviejs/graphql-server-servie/issues"
  },
  "homepage": "https://github.com/serviejs/graphql-server-servie",
  "devDependencies": {
    "@types/graphql": "^0.11.5",
    "@types/jest": "^22.1.0",
    "@types/node": "^8.0.47",
    "graphql": "^0.11.7",
    "jest": "^21.2.1",
    "rimraf": "^2.5.4",
    "servie": "^0.2.5",
    "throwback": "^2.0.0",
    "ts-jest": "^21.1.4",
    "tslint": "^5.0.0",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^2.2.1"
  },
  "peerDependencies": {
    "servie": "^0.2.0",
    "graphql": "*"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest/preprocessor.js"
    },
    "testRegex": "src/.*\\.(?:test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "get-body": "^1.0.2",
    "graphql-server-core": "^1.2.0",
    "graphql-server-module-graphiql": "^1.2.0",
    "servie-send": "0.0.1"
  }
}
